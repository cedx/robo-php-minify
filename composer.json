{
  "description": "Robo plug-in minifying PHP source code by removing comments and whitespace.",
  "homepage": "https://dev.belin.io/robo-php-minify",
  "license": "MIT",
  "name": "cedx/robo-php-minify",
  "type": "robo-tasks",
  "authors": [
    {"email": "cedric@belin.io", "homepage": "https://belin.io", "name": "CÃ©dric Belin"}
  ],
  "autoload": {
    "psr-4": {"Robo\\PhpMinify\\": "lib/"}
  },
  "autoload-dev": {
    "psr-4": {"Robo\\PhpMinify\\": "test/"}
  },
  "config": {
    "optimize-autoloader": true
  },
  "funding": [
    {"type": "patreon", "url": "https://www.patreon.com/cedx"}
  ],
  "keywords": [
    "build",
    "compress",
    "minify",
    "php",
    "robo",
    "task"
  ],
  "require": {
    "php": ">=7.4.0",
    "ext-mbstring": "*",
    "ext-spl": "*",
    "cedx/enum": "^8.0.0",
    "cedx/which": "^8.0.0",
    "consolidation/robo": "^2.0.3",
    "guzzlehttp/guzzle": "^6.5.2",
    "symfony/finder": "^4.4.5 || ^5.0.5",
    "symfony/process": "^4.4.5 || ^5.0.5",
    "webmozart/path-util": "^2.3.0"
  },
  "require-dev": {
    "cedx/coveralls": "^12.0.0",
    "cedx/phpunit-expect": "^0.10.0",
    "league/container": "^2.4.1",
    "phpstan/phpstan": "^0.12.14",
    "symfony/console": "^4.4.5 || ^5.0.5"
  },
  "scripts": {
    "coverage": "robo coverage",
    "test": "robo test"
  },
  "support": {
    "docs": "https://dev.belin.io/robo-php-minify/api",
    "issues": "https://github.com/cedx/robo-php-minify/issues"
  }
}
